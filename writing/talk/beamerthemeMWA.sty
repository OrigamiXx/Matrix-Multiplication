%% Corrects vertical alignment of title.
\newenvironment{myframe}[1]{
\begin{frame}{\vspace{1mm}\bf{#1}}
\vspace{1ex}
}
{
\end{frame}
}

\newenvironment{fragileframe}[1]{
\begin{frame}[fragile]
\frametitle{\vspace{1mm}\bf{#1}}
\vspace{1ex}
}
{
\end{frame}
}

\newenvironment{myblock}[1]
{
  \def \tempwidth {\linewidth}
  \addtolength{\tempwidth}{0ex}%-1ex}

  \vskip.75ex
  \begin{minipage}{\tempwidth}
  \begin{beamercolorbox}[rounded=false,shadow=false,leftskip=0cm,colsep*=.75ex]{block title}%
    \usebeamerfont*{block title}#1
  \end{beamercolorbox}%
  {\ifbeamercolorempty[bg]{block body}{}{\nointerlineskip\vskip-0.5pt}}%
  \usebeamerfont{block body}%
  \begin{beamercolorbox}[rounded=false,shadow=false,colsep*=.75ex,sep=.5ex,vmode]{block body}%
    \ifbeamercolorempty[bg]{block body}{\vskip-.25ex}{\vskip-.75ex}\vbox{}%
    
  }{ \end{beamercolorbox}
%  \vskip.75ex
%  \medskip
\smallskip
  \end{minipage}
}

\newcommand*{\squareref}[1]{
  \hbox{%
    \usebeamerfont*{item projected}%
    \usebeamercolor[bg]{item projected}%
    \vrule width2.25ex height1.85ex depth.4ex%
    \hskip-2.25ex%
    \hbox to2.25ex{%
      \hfil%
      \color{fg}\ref{#1}%
      \hfil}%
  }%
}

\newcommand{\hlbox}[1]{\colorbox{\maincolor}{\color{white}#1}}
\newcommand{\hlword}[1]{\structure{\bf{#1}}}

\newcommand*{\ballref}[1]{%
    \begin{pgfpicture}{-1ex}{-0.65ex}{1ex}{1ex}
    \usebeamercolor[fg]{item projected}
    {\pgftransformscale{1.75}\pgftext{\normalsize\pgfuseshading{bigsphere}}}
    {\pgftransformshift{\pgfpoint{0pt}{0.5pt}}
      \pgftext{\usebeamerfont*{item projected}\ref{#1}}}
  \end{pgfpicture}}%


